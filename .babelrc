{
  "presets": [
    [
      "env",
      {
        "targets": {
          "browsers": [
            "last 3 versions",
            "safari >= 7",
            "not ie <= 8"
          ]
        }
      }
    ],
    "react","stage-0","stage-1","stage-2","stage-3"
  ],
  "plugins": [
    [
      "import",
      {
        "libraryName": "antd",
        "style": true,//"css",//"style": true 会导入 LESS/Sass 源文件
        "cacheDirectory": true
      }
    ]
  ]
}